## ---------------------------------------------------------------------------
#set ($record = $result.Detail)
## ---------------------------------------------------------------------------
<div id="messageDiv"></div>
<div class="auiPopup">
<h2>$l10n.TIMELINE_SAYING_LIKE</h2>
    <div class="auiListWrapper">
		<ul class="userList">
#foreach ($record2 in $record.LikeList)
	<li>
		<div class="content">
			<div class="avatar">
#if(${record2.hasPhoto()})
				<td class="caption"><img style="border-radius: 3px;" src="$!utils.escapeXML($jslink.getTemplate("FileuploadFacePhotoScreen").addQueryData("uid", ${record2.UserId}).addQueryData("t", ${record2.PhotoModified}))" alt="${record2.UserName}" width="50" height="50" /></td>
#else
      <td class="caption"><img style="border-radius: 3px;" src="images/common/avatar_default3.png" alt="${record2.UserName}" width="50" height="50" /></td>
#end
</div>
<div class="name">
#ALtditem($record2.UserName)
</div>
</div>
</li>
#end
</ul>
</div>
#ALbuttonheader()
<p class="rightBtn pt10" onclick="aipo.common.hideDialogSub();" ><button>$l10n.TIMELINE_CLOSE</button></p>
#ALbuttonfooter()
</div>

